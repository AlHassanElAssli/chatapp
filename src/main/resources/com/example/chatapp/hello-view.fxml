<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.chatapp.ChatController">
   <ListView fx:id="messageListView" VBox.vgrow="ALWAYS" prefWidth="600.0" />
   <HBox prefWidth="600.0">
      <TextField fx:id="messageField" HBox.hgrow="ALWAYS" />
      <Button fx:id="sendButton" mnemonicParsing="false" onAction="#addMessage" text="Send" />
   </HBox>
</VBox>
